DirectoryIndex index.cgi/
RewriteEngine On
RewriteCond %{HTTP_HOST} ^(www\.)?sandabu\.sakura\.ne\.jp$ [NC,OR]
RewriteCond %{HTTP_HOST} ^www\.sandabu\.net$ [NC]
RewriteRule .* http://sandabu.net%{REQUEST_URI} [R=301,L]

RewriteBase /
RewriteCond %{REQUEST_URI} !(css|js|font|uploads|image|console_styles|index.cgi)
RewriteRule ^(.*)$ index.cgi/$1 [QSA,L]
